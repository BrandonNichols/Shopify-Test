{% assign imageList =
'alvan-nee-eoqnr8ikwFE-unsplash,charles-deluvio-K4mSJ7kc0As-unsplash,richard-brutyo-Sg3XwuEpybU-unsplash'
| split: ',' %}
{% assign productCollection = collections['all'] %}
{% assign
imageIndex = 0%}

<div class="hero-image">
